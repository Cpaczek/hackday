{
  "name": "Hack Day",
  "version": "1.0",
  "description": "Fixes UI bugs in workday",
  "permissions": [
    "tabs",
    "https://*.myworkday.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "HackDay"
  },
  "manifest_version": 2
}